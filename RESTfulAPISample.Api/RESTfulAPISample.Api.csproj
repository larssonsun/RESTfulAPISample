<Project Sdk="Microsoft.NET.Sdk.Web">
  <ItemGroup>
    <ProjectReference Include="..\RESTfulAPISample.core\RESTfulAPISample.core.csproj"/>
    <ProjectReference Include="..\RESTfulAPISample.infrastructure\RESTfulAPISample.Infrastructure.csproj"/>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="9.0.0"/>
    <PackageReference Include="FluentValidation" Version="8.6.1"/>
    <PackageReference Include="FluentValidation.AspNetCore" Version="8.6.1"/>
    <PackageReference Include="Marvin.Cache.Headers" Version="4.1.0"/>
  </ItemGroup>
  <ItemGroup>  
      <PackageReference Include="MessagePack" Version="2.0.323"/>
  </ItemGroup>
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>

    <!-- 下面这段无用 可删除-->
     <!-- 
      加入自定义条件编译符号
      DISTRIBUTEDCACHE 分布式缓存（默认存储于内存）
      LOCALMEMORYCACHE 服务器本机内存缓存
      -->
    <DefineConstants>$(DefineConstants);LOCALMEMORYCACHE</DefineConstants>
    <!-- 设置自定义条件编译符号的值 这里只影响 ItemGroup Condition-->
    <!-- 上面这段无用 可删除-->

    <DISTRIBUTEDCACHE>False</DISTRIBUTEDCACHE>
  </PropertyGroup>
</Project>